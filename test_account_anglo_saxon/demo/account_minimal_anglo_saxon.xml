<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data >

        <record id="stk_output" model="account.account">
            <field name="code">X11020</field>
            <field name="name">To Be Invoiced - (test)</field>
            <field ref="account.cas" name="parent_id"/>
            <field name="type">other</field>
            <field name="user_type" ref="account.data_account_type_asset"/>
        </record>

        <record id="stk_input" model="account.account">
            <field name="code">X11120</field>
            <field name="name">Awaiting Supplier Invoice - (test)</field>
            <field ref="account.cli" name="parent_id"/>
            <field name="type">other</field>
            <field name="user_type" ref="account.data_account_type_liability"/>
        </record>

        <record id="crd_price_diff" model="account.account">
            <field name="code">X21020</field>
            <field name="name">Creditor Price Difference - (test)</field>
            <field ref="account.cos" name="parent_id"/>
            <field name="type">other</field>
            <field name="user_type" ref="account.data_account_type_expense"/>
        </record>

        <!--
        Product category accounts, default parameters
        -->

        <record id="property_account_creditor_price_diff_categ" model="ir.property">
            <field name="name">property_account_creditor_price_difference_categ</field>
            <field name="fields_id" search="[('model','=','product.category'),('name','=','property_account_creditor_price_difference_categ')]"/>
            <field eval="'account.account,'+str(ref('test_account_anglo_saxon.crd_price_diff'))" name="value"/>
            <field name="company_id" ref="base.main_company"/>
        </record>

        <record id="stock.property_stock_account_input_categ" model="ir.property">
            <field eval="'account.account,'+str(ref('test_account_anglo_saxon.stk_input'))" name="value"/>
        </record>
        <record id="stock.property_stock_account_output_categ" model="ir.property">
            <field eval="'account.account,'+str(ref('test_account_anglo_saxon.stk_output'))" name="value"/>
        </record>

        <record id="stock.property_stock_account_output_prd" model="ir.property">
            <field eval="False" name="value"/>
        </record>
        <record id="stock.property_stock_account_input_prd" model="ir.property">
            <field eval="False" name="value"/>
        </record>

        <!--
        Test Products Setup
        -->

        <record id="product_template_tas_1" model="product.template">
            <field name="name">Test Consumable Product (tas)</field>
            <field name="categ_id" ref="product.product_category_3"/>
            <field name="list_price">450.0</field>
            <field name="standard_price">300.0</field>
            <field name="type">consu</field>
            <field name="uom_id" ref="product.product_uom_unit"/>
            <field name="uom_po_id" ref="product.product_uom_unit"/>
            <field name="cost_method">average</field>
        </record>
        <record id="product_product_tas_1" model="product.product">
            <field name="product_tmpl_id" ref="product_template_tas_1"/>
            <field name="valuation">real_time</field>
            <field name="default_code">TAS1</field>
        </record>

        <record id="product_template_tas_2" model="product.template">
            <field name="name">Test Stock Product (tas)</field>
            <field name="categ_id" ref="product.product_category_3"/>
            <field name="list_price">45.0</field>
            <field name="standard_price">30.0</field>
            <field name="type">product</field>
            <field name="uom_id" ref="product.product_uom_unit"/>
            <field name="uom_po_id" ref="product.product_uom_unit"/>
            <field name="cost_method">average</field>
        </record>
        <record id="product_product_tas_2" model="product.product">
            <field name="product_tmpl_id" ref="product_template_tas_1"/>
            <field name="valuation">real_time</field>
            <field name="default_code">TAS2</field>
        </record>

        <record id="product_template_tas_3" model="product.template">
            <field name="name">Test Service Product (tas)</field>
            <field name="categ_id" ref="product.product_category_3"/>
            <field name="list_price">45.0</field>
            <field name="standard_price">30.0</field>
            <field name="type">service</field>
            <field name="uom_id" ref="product.product_uom_unit"/>
            <field name="uom_po_id" ref="product.product_uom_unit"/>
            <field name="cost_method">average</field>
        </record>
        <record id="product_product_tas_3" model="product.product">
            <field name="product_tmpl_id" ref="product_template_tas_1"/>
            <field name="valuation">real_time</field>
            <field name="default_code">TAS3</field>
        </record>

    </data>
</openerp>